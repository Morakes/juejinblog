{
  "name": "yep",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint src --fix"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.1",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@vueuse/core": "^6.7.4",
    "axios": "0.21.4",
    "element-plus": "1.1.0-beta.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "font-awesome": "^4.7.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.1",
    "qs": "^6.10.1",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "vue": "^3.2.16",
    "vue-router": "4",
    "vuex": "4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.4.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  },
  "husky": {
    "hooks": {
        "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
      "src*/**/*.ts": [
          "prettier --config .prettierrc.js --write",
          "eslint --fix",
          "git add"
      ],
      "src*/**/*.json": [
          "prettier --config .prettierrc.js --write",
          "eslint",
          "git add"
      ]
  }
}
